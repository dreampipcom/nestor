{
  "name": "@dreampipcom/nestor/js",
  "version": "0.0.4",
  "description": "DreamPip's Intelligence Javascript SDK.",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "format": "prettier --write --parser typescript '**/*.{ts,tsx}'",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore --fix",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test-watch": "vitest",
    "prepare": "husky",
    "ci:format": "npm run format && npm run lint",
    "snyk-protect": "snyk-protect",
    "prepublishOnly": "npm version patch && git add . && git commit -m 'dp(pkg) patch' && git push && rm package-lock.json && npm install && npm run build"
  },
  "author": "Angelo Reale",
  "license": "ISC",
  "devDependencies": {
    "@snyk/protect": "latest",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.9",
    "vite-plugin-dts": "^3.8.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.0"
  },
  "peerDependencies": {},
  "snyk": true
}
